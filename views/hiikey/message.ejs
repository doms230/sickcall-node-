    </ul>
</div>

<!-- Messages -->

<div class="page-header">
    <% for (var i = 0; i < data.length  ; i++ ) { %>
    <div class="container-fluid">
        <div class="media">
            <div class="media-left">
                <img class="img-circle" src= <%=data[i].image%> alt="..." width="64" height="64">
            </div>
            <div class="media-body">
                <h4 class="media-heading"> <%=data[i].name%> <small> <%=data[i].createdAt%> </small></h4>
                    <%=data[i].message%>
            </div>
        </div>
        </br>
    </div>

    <% }%>
</div>

<!--- End Messages -->
