</div>

<!-- tab jaunts -->

<div  id="locationDiv" class="page-header" hidden="yes">
</div>

<div class="container-fluid">
    <ul class="nav nav-tabs">
        <li id="messages" role="presentation" class="active"><a>Messages</a></li>
        <li id="rsvps" role="presentation"><a>Guests</a></li>
    </ul>
</div>

<div id="messageDiv" class="page-header" hidden="yes">
</div>

<div id="rsvpDiv" class="page-header" hidden="yes">
</div>

<div id="rsvpButton" class="page-header" hidden="yes" >
    <div class="container-fluid">
        <button  type="button" class="btn btn-danger btn-lg btn-block">JOIN</button>
    </div>
</div>

<div id="alertDiv" class="page-header" hidden="yes">
</div>

<ul id="messageBarDiv" class=" navbar-fixed-bottom" hidden="yes">
    <div class="input-group">
        <input id="usermsg" type="text" class="form-control" placeholder= "Message"  >
      <span class="input-group-btn">
        <button id="sendmsg" class="btn btn-default" type="button">Send</button>
      </span>
    </div><!-- /input-group -->
</ul>

<div id="modelRequests" class="modal fade" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title"> Access Requests</h4>
            </div>
            <div id="requestBody" class="modal-body">

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>

            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

<!-- Edit event modal  -->
<div id="modalEdit" class="modal fade" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Edit Event</h4>
            </div>
            <div id="requestBodyEdit" class="modal-body">
                <img src="#" id="inputImage"  alt="Upload Event Image" class="img-thumbnail" width="100" height="150">

                </br>
                </br>

                <input type="file" id="editEventImage" />

                <h3>Title <small class="astericks">*</small></h3>
                <input id="inputTitle" type="title" class="form-control" placeholder="Make it Short">

                </br>

                <h3>Address <small class="astericks">*</small></h3>
                <input id="inputAddress" type="text" class="form-control" placeholder="Event location">

                <h3>Description</h3>
                <textarea id="inputDescription" placeholder="Event details, band links, etc." class="form-control" rows="5"></textarea>

                <!-- <input placeholder="start date" id="date" type="text" class="form-control">

                 <input placeholder="start time" id="time" type="text" class="form-control"> -->

                </br>

                <h3>Starts <small class="astericks">*</small></h3>

                <form class="form-inline">
                    <div class="form-group">
                        <label class="sr-only" for="exampleInputEmail3">Start Date</label>
                        <input class="form-control" id="inputStartDate" placeholder="Start Date">
                    </div>
                    <div class="form-group">
                        <label class="sr-only" for="exampleInputPassword3">Start Time</label>
                        <input class="form-control" id="inputStartTime" placeholder="Start Time">
                    </div>
                </form>

                <h3>Ends <small class="astericks">*</small></h3>

                <form class="form-inline">
                    <div class="form-group">
                        <label class="sr-only" for="exampleInputEmail3">End Date</label>
                        <input class="form-control" id="inputEndDate" placeholder="End Date">
                    </div>

                    <div class="form-group">
                        <label class="sr-only" for="exampleInputPassword3">End Time</label>
                        <input class="form-control" id="inputEndTime" placeholder="End Time">
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button id="updateEvent" type="button" class="btn btn-default astericks">Update Event Info</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

<!-- Modal if user isn't logged in -->
<div class="modal fade" id="registerModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Sign In / Sign Up to RSVP</h4>
            </div>
            <div class="modal-body">
                <ul class="nav nav-pills">
                    <li href="#" id="signupButton" role="presentation" class="active"><a>Sign Up</a></li>
                    <li href="#" id="signinButton" role="presentation"><a>Sign In</a></li>
                    <li href="#" id="forgotPasswordButton" role="presentation"><a>Forgot Password</a></li>
                </ul>

                <div class="container-fluid" id="signin" hidden="yes"  >
                    <form  class="form-signin" >
                        <h3 class="form-signin-heading"> Sign In</h3>
                        <input type="text" id="inputUsername" class="form-control" placeholder="Username" required autofocus>
                        <label for="inputPassword" class="sr-only">Password</label>
                        <input type="password" id="inputPassword" class="form-control" placeholder="Password" required>
                    </form>
                    <button  id="signinAction" class="btn btn-lg btn-default btn-block" type="submit">Sign in</button>

                </div>

                <div class="container-fluid" id="signup"  >
                    <form  class="form-signin">
                        <h3 class="form-signin-heading">Sign Up</h3>
                        <input type="text" id="signupUsername" class="form-control" placeholder="Username" required autofocus>
                        <input type="email" id="signupEmail" class="form-control" placeholder="Email" required autofocus>
                        <input type="password" id="signupPassword" class="form-control" placeholder="Password" required>
                    </form>
                    <button id="signupAction" class="btn btn-lg btn-default btn-block" type="submit">Sign up</button>

                </div>
                <div class="container-fluid" id="forgotpassword" hidden="yes" >
                    <form  class="form-signin">
                        <h3 class="form-signin-heading">Forgot Password</h3>
                        <input type="email" id="forgotpasswordEmail" class="form-control" placeholder="Email" required autofocus>
                    </form>
                    <button id="forgotPasswordAction" class="btn btn-lg btn-default btn-block" type="submit">Reset Password</button>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- forgot password -->

<div class="modal fade" id="navForgotPasswordModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Forgot Password</h4>
            </div>
            <div class="modal-body">
                <div class="container-fluid" id="f" >
                    <form  class="form-signin">
                        <input type="email" id="navForgotpasswordEmail" class="form-control" placeholder="Email" required autofocus>
                    </form>
                    <button id="navForgotPasswordAction" class="btn btn-lg btn-default btn-block" type="submit">Reset Password</button>
                </div>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<!-- profile modal  -->
<div class="modal fade" id="profileModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Last Step! Update Your Profile</h4>
            </div>
            <div class="modal-body">

                <div id="fb-root"></div>
                <div  id="locationDiv" class="page-header" >

                    <div id="userInfo" class="container-fluid">
                        <img src="#" id="profileImage"  alt="Upload Profile Image" class="img-circle" width="100" height="100">

                        </br>
                        </br>

                        <input type="file" id="newProfilePhoto" />

                        </br>
                        </br>

                        <!--<button id="updatePhoto" class="btn btn-sm btn-default" type="submit">Update Profile Picture</button> -->

                        <form class="form-signin">
                            <h4 id="username" class="form-signin-heading"></h4>
                            <input type="text" id="inputName" class="form-control" placeholder="Name" required autofocus>
                            <label for="inputPassword" class="sr-only">Password</label>
                            <input type="text" id="inputNumber" class="form-control" placeholder="Phone Number" required>
                        </form>

                    </div>

                </div>
            </div>

            <div class="modal-footer">
                <button id="updateInfo" class="btn btn-lg btn-default btn-block" type="submit">Update Info</button>
            </div>

        </div>
    </div>
</div>

<!-- Remove Event modal -->

<div class="modal fade" id="removeEventModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Cancel <%=title%> ? </h4>
            </div>
            <div class="modal-body">
                <div class="container-fluid" id="f" >
                    <button id="removeEventAction" type="button" class="btn btn-default" >Yes</button>
                </div>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>


